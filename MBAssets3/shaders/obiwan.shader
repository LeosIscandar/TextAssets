// TPM

models/players/obiwan_ep1/boots
{
	{
		map models/players/obiwan_ep1/boots
		rgbGen lightingDiffuse
	}
	{
		map models/players/obiwan_ep1/boots_spec
		blendFunc GL_SRC_ALPHA GL_ONE
		alphaGen lightingSpecular
		detail
	}
}

