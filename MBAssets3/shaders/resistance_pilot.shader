models/players/resistance_pilot/head_diffuse
{
	q3map_nolightmap
    {
        map models/players/resistance_pilot/head_diffuse
        rgbGen lightingDiffuse
        // alphaFunc GE128
    }
    {
        map models/players/resistance_pilot/head_spec
        blendFunc GL_ONE_MINUS_DST_COLOR GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

