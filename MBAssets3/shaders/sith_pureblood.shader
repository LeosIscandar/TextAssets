models/players/sith_pureblood/arms
{
	{
		map models/players/sith_pureblood/arms
		rgbGen lightingDiffuse
	}
	{
		map models/players/sith_pureblood/arms-spec
		blendFunc GL_SRC_ALPHA GL_ONE
